// made into a function so that you can use any number.
function fizzBuzz(num){ 
  	this.num = num;
    // this is my for loop that counts to num
    for (var i = 1; i < num; i++)
      // if i is divisable by 15 then it turns to FizzBuzz
      if(i % 15 === 0)
        console.log("FizzBuzz");
      // if i is divisable by 5 then it turns to Buzz
      else if(i % 5 === 0)
        console.log("Buzz");
      // if i is divisable by 3 then it turns to Fizz
      else if(i % 3 === 0)
        console.log("Fizz");
      // if i is any other number then print that number 
      else
        console.log(i);
}
fizzBuzz(16);  
